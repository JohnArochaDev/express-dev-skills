<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Sexy <%= title %></h1>
    <p>Welcome to my dream <%= title.toLowerCase() %></p>
    <ul>
      <% car.forEach(function(car) { %>
        <li>
          <form action="/cars/<%= car.id %>?_method=DELETE"
              method="POST"
              class="df"
          >
            <button type="submit">X</button>
          </form>
          <a href="/cars/<%= car.id %>"><%= car.name %></a>
        </li>
        <% }) %>
    </ul>
    <a href="/cars/new">New Car</a><br>
  </body>
</html>